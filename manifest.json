{
    "id": "SmiloQuake",
    "version": "1.0.2",
    "title": "Smilo Quake",
    "subTitle": "Shoot, kill and earn SmiloPay",
    "manifestUrl": "https://s3-eu-west-1.amazonaws.com/smilo-quake/manifest.json",
    "description": "The classic first person shooter Quake II now reimagined by the Smilo team! Win SmiloPay by playing against strangers or your friends. Compete in high stake matches to earn even more SmiloPay!",
    "tags": ["shooter", "old school", "fps"],
    "thumbnailUrl": "https://s3-eu-west-1.amazonaws.com/smilo-quake/art/thumbnail.png",
    "art": [
        {
            "type": "image",
            "url": "https://s3-eu-west-1.amazonaws.com/smilo-quake/art/screen1.png"
        },
        {
            "type": "image",
            "url": "https://s3-eu-west-1.amazonaws.com/smilo-quake/art/screen2.png"
        },
        {
            "type": "image",
            "url": "https://s3-eu-west-1.amazonaws.com/smilo-quake/art/screen3.png"
        },
        {
            "type": "image",
            "url": "https://s3-eu-west-1.amazonaws.com/smilo-quake/art/screen4.png"
        },
        {
            "type": "image",
            "url": "https://s3-eu-west-1.amazonaws.com/smilo-quake/art/screen5.png"
        }
    ],
    "platforms": [
        {
            "name": "darwin",
            "downloadUrl": "https://s3-eu-west-1.amazonaws.com/smilo-quake/darwin_x86_64.tar.gz",
            "baseDir": ".",
            "binary": "./SmiloQuake",
            "args": "+set name ${PROFILE_NAME} ${GAME_SERVERS}"
        },
        {
            "name": "win32",
            "downloadUrl": "https://s3-eu-west-1.amazonaws.com/smilo-quake/windows_x86_64.tar.gz",
            "baseDir": ".",
            "binary": "SmiloQuake.exe",
            "args": "+set name ${PROFILE_NAME} ${GAME_SERVERS}"
        }
    ],
    "retainWhenUpdating": [
        "baseq2/config.cfg",
        "baseq2/q2config.cfg"
    ],
    "params": {
        "GAME_SERVERS": {
            "url": "https://s3-eu-west-1.amazonaws.com/smilo-quake/servers.lst"
        }
    }
}